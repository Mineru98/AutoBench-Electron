{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.0",
    "chart.js": "^2.9.3",
    "electron-is-dev": "^1.1.0",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "scripts": {
    "start": "concurrently \"BROWSER=none yarn start:react\" \"wait-on http://localhost:3000 && electron .\"",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon={icon path .icns} --prune=true --out=release-builds --electron-version=7.1.7",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon={icon path .ico} --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"AutoBenchClient\" --electron-version=7.1.7",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icon-512-nocolor.png} --prune=true --out=release-builds --electron-version=7.1.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.1.1",
    "electron-prebuilt": "^1.4.13",
    "electron-winstaller": "^4.0.0",
    "wait-on": "^3.3.0"
  },
  "main": "public/electron.js"
}
